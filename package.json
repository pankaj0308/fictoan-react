{
    "name": "fictoan",
    "private": true,
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "scripts": {
        "bootstrap": "lerna bootstrap --use-workspaces",
        "build": "lerna exec --scope fictoan-react yarn build",
        "build:all": "lerna run --stream build",
        "setup": "yarn && yarn bootstrap && yarn build",
        "storybook": "start-storybook -p 6006",
        "storybook-build": "build-storybook -c .storybook -o public",
        "dev": "lerna exec --scope fictoan-docs yarn dev",
        "build:app": "lerna exec --scope fictoan-docs yarn build",
        "start": "lerna exec --scope fictoan-docs yarn start",
        "postinstall": "bash scripts/postinstall.sh",
        "prepare": "husky install",
        "watch": "lerna exec --scope fictoan-react yarn watch"
    },
    "resolutions": {
        "@types/react": "17.0.2",
        "@types/react-dom": "17.0.2"
      },
    "devDependencies": {
        "@babel/cli": "7.18.10",
        "@babel/core": "7.18.13",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/preset-env": "7.18.10",
        "@babel/preset-react": "7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@storybook/addon-actions": "6.3.7",
        "@storybook/addon-essentials": "6.3.7",
        "@storybook/addon-links": "6.3.7",
        "@storybook/react": "6.3.7",
        "@storybook/storybook-deployer": "2.8.12",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "28.1.3",
        "babel-loader": "8.2.5",
        "eslint": "8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.8",
        "jest-environment-jsdom": "^28.1.3",
        "lerna": "5.4.3",
        "prettier": "^2.7.1",
        "react-test-renderer": "17.0.2",
        "stylelint": "14.11.0",
        "stylelint-config-get-off-my-lawn": "2.0.0",
        "stylelint-config-styled-components": "0.1.1",
        "stylelint-processor-styled-components": "1.10.0",
        "typescript": "^4.4.3",
        "polished": "^4.1.3",
        "typescript-plugin-styled-components": "^2.0.0",
        "husky": "^7.0.1",
        "jest": "^27.2.1",
        "jest-dom": "^4.0.0",
        "@types/lodash-es": "^4.17.5",
        "@types/react": "^17.0.2",
        "@types/react-syntax-highlighter": "^13.5.1",
        "@types/styled-components": "^5.1.11",
        "lint-staged": "^11.1.1",
        "@types/node": "18.11.5",
        "ts-loader": "^9.4.1"
    },
    "dependencies":{
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "styled-components": "5.3.0"
    }
    
}
